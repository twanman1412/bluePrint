# bundle.manifest for std/core
bundle: std.core
version: 1.0.0
exports:
  # Core object model (from objects sub-bundle)
  - Object
  - Equatable
  - Comparable
  - Hashable
  - Clonable
  - Printable
  
  # Runtime blueprints (from runtime sub-bundle)
  - Application
  - Exception
  - RuntimeException

dependencies:
  # Core depends on its sub-bundles
  - std.core.objects
  - std.core.runtime
